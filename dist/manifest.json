{
    "name": "Facebook Messenger - Flat IRC Inspired Theme",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "An IRC inspired theme for Facebook Messenger (messenger.com)",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.messenger.com/*"
            ],
            "css": [
                "styles.min.css"
            ],
            "js": [
                "scripts.min.js"
            ]
        }
    ]
}
